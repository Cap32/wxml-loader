{
  "name": "wxml-loader",
  "description": "wxml loader for webpack",
  "version": "0.0.0-3",
  "main": "lib/index",
  "files": [
    "*.md",
    "bin",
    "lib"
  ],
  "scripts": {
    "start": "yarn test -- -w --watch-extensions js",
    "test": "NODE_ENV=development mocha -t 10000 --compilers js:babel-core/register test",
    "prebuild": "rimraf lib",
    "build:watch": "babel src -d lib -w",
    "build": "babel src -d lib",
    "preversion": "yarn test && yarn build"
  },
  "author": "cap32",
  "repository": "cap32/wxml-loader",
  "keywords": [],
  "licenses": "MIT",
  "dependencies": {
    "loader-utils": "^1.1.0",
    "minimize": "^2.1.0",
    "sax": "^1.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-es2015-parameters": "^6.18.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "cross-env": "^4.0.0",
    "eslint": "^2.4.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4"
  }
}
